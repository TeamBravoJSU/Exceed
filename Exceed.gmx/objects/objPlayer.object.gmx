            <string>if (health &lt; 1){                 // If health has fallen too low
    audio_play_sound(explosion,0,0)
    instance_destroy();              // Destroy the calling instance
}
if health &gt;= maxHealth{health = maxHealth}

if keyboard_check(vk_up)|| gamepad_button_check(0,gp_padu){
    y -= 10
}


if keyboard_check(vk_down)|| gamepad_button_check(0,gp_padd){
 y += 10
 }
if keyboard_check(vk_left)|| gamepad_button_check(0,gp_padl){
     x -= 10
     turnDir = "left"
}
if keyboard_check(vk_right)|| gamepad_button_check(0,gp_padr){
     x += 10
     turnDir = "right"
}
if gamepad_button_check_released(0,gp_padl){
turnDir = "leftToCenter"
}
if gamepad_button_check_released(0,gp_padr){
turnDir = "rightToCenter"
}

x = median(80, x, room_width-80);
y = median(80, y, room_height-120)

if (keyboard_check(ord("X")))|| gamepad_button_check(0,gp_face1){
if (alarm[0] &lt; 0) {
    alarm[0] = room_speed * 0.25; 
    instance_create(x-30, y, objPlayerBullet)
    instance_create(x+30, y, objPlayerBullet)
    audio_play_sound(laser_default,0,0)
    }
}

if gamepad_button_check_pressed(0,gp_face2){
    if global.bomb &gt; 0{
        instance_create(x,y , objBomb)
        with (objEnemy)
        {instance_destroy()}
        with (enemy2)
        {instance_destroy()}
        with (objEnemyBullet)
        {instance_destroy()}
        with (objEnemyBullet2)
        {instance_destroy()}
    }
    global.bomb--
}

</string>
